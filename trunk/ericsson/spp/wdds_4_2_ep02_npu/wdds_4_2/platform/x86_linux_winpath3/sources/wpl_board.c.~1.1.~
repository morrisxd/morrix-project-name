/* TBD win_num_winpaths - revert to a BSP function */
WP_U32 win_num_winpaths(void) {	return 1;};

/******************************************************************/
/*                       API Functions                            */
/******************************************************************/

/*****************************************************************************
 *
 * Function: WPL_GetWinPaths
 *
 * Purpose: Return number of WinPaths in system
 *
 * Inputs:
 *     None
 *
 * Outputs:
 *     None
 *
 * Return Value:
 *     1
 *
 * Called by:
 *     WDDI
 *
 ****************************************************************************/

WP_U32 WPL_GetWinPaths(void)
{
	return win_num_winpaths();
}


